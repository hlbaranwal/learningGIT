app.controller('createCustomerCtrl',function($scope,$http){
	$scope.createPerson=function(){
        var sendData =[{firstName:$scope.firstName,lastName:$scope.lastName,emailId:$scope.emailId}];
        $http.post('https://localhost:8443/partymgr/control/createPerson',{data:sendData}).success(function(response){
        $scope.dataReceived=response;
        });
    }
    $scope.myf=function(){
        alert("Customer is created!");
    }
});

app.controller('customerCtrl',function($scope,$http){
    $http.post('https://localhost:8443/ofbizdemo/control/findAllPerson')
        .success(function(response){
            $scope.dataReceived=response.mylist;
          });
});
